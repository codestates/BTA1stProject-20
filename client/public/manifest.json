{
    "manifest_version": 3,
    "name": "$IMX Wallet Chrome Extension",
    "description": "A Chrome Extension Wallet for $IMX",
    "version": "0.1.0",
    "action": {
        "default_popup": "index.html",
        "default_title": "$IMX Wallet Chrome Extension"
    },
    "permissions": [
        "storage"
    ],
    "icons": {
        "16": "imx_icon_16.png",
        "32": "imx_icon_32.png",
        "48": "imx_icon_48.png",
        "128": "imx_icon_128.png",
        "334": "imx_icon_334.png"
    }
}
